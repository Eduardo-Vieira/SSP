<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
						http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0">

	<portlet>
		<portlet-name>MyGPS</portlet-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/spring/portlet/MyGPS-portlet.xml</value>
		</init-param>
		<!-- This value is to 0 in dev, can set to cache at a positive value in seconds 
			for caching, or -1 to never expire cache -->
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>MyGPS</title>
		</portlet-info>
		<security-role-ref>
			<role-name>AccessType_ARC</role-name>
			<role-link>AccessType_Academic Resource Center</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_CNSL</role-name>
			<role-link>AccessType_Counseling Services</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_DIS</role-name>
			<role-link>AccessType_Disability</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_DSW</role-name>
			<role-link>AccessType_Displaced Workers</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_EAL</role-name>
			<role-link>AccessType_Early Alert</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_ESL</role-name>
			<role-link>AccessType_English as a Second Language</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_FFC</role-name>
			<role-link>AccessType_Fast Forward</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_ILP</role-name>
			<role-link>AccessType_Individualized Learning Plan</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_MGR</role-name>
			<role-link>AccessType_Manager</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>AccessType_STAFF</role-name>
			<role-link>AccessType_Staff</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Academic_Counselor_Advisor</role-name>
			<role-link>Role_Academic Counselor or Advisor</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Counselor</role-name>
			<role-link>Role_Counselor</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Faculty</role-name>
			<role-link>Role_Faculty</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Developer</role-name>
			<role-link>Role_Developer</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Management</role-name>
			<role-link>Role_Management</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Super_User</role-name>
			<role-link>Role_Super User</role-link>
		</security-role-ref>
		<security-role-ref>
			<role-name>Role_Support_Staff</role-name>
